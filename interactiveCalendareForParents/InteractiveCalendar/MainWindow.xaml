<Window x:Class="InteractiveCalendar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InteractiveCalendar"
        mc:Ignorable="d"
        Title="Interactive Callendar For Parents Of Newborns and Infint Children" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="39*"/>
            <RowDefinition Height="41*"/>
        </Grid.RowDefinitions>
        <Button x:Name="button" Content="Start" HorizontalAlignment="Left" Height="37" Margin="40,19,0,0" VerticalAlignment="Top" Width="74" Click="button_Click" RenderTransformOrigin="-1.763,-2.186"/>
        <Button x:Name="button1" Content="Quit" HorizontalAlignment="Left" Height="37" Margin="390,19,0,0" VerticalAlignment="Top" Width="71" Click="button1_Click"/>
        <Menu x:Name="menu" HorizontalAlignment="Left" Height="3" Margin="517,0,0,0" Grid.Row="1" VerticalAlignment="Top" Width="0"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="62" Margin="40,22,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="200" TextChanged="textBox_TextChanged"/>
        <Button x:Name="button2" Content="Enter" HorizontalAlignment="Left" Height="36" Margin="40,89,0,0" Grid.Row="1" VerticalAlignment="Top" Width="91" Click="button2_Click"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="62" Margin="290,22,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>

    </Grid>
</Window>
